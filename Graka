package spiele;

public class Graka {
    
    
public static void main(String []args) {
 
GrakaInfo Hardware = new GrakaInfo();    
GrakaInfo Hardware0 = new GrakaInfo("gigabyte", 240, false, 1000, 3072);
GrakaInfo Hardware1 = new GrakaInfo("Asus", 260, true, 1100, 1024);
GrakaInfo Hardware2 = new GrakaInfo("Sapphire", 260, true, 1050, 2048);

System.out.println(Hardware);    
System.out.println(Hardware0);
System.out.println(Hardware1);
System.out.println(Hardware2);

}

       
    
    
}
